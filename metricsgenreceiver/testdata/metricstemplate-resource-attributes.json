{
  "resourceMetrics": [
    {
      "resource": {
        "attributes": [
          {
            "key": "host.name",
            "value": {
              "stringValue": "host-{{.InstanceID}}"
            }
          },
          {
            "key": "host.ip",
            "value": {
              "arrayValue": {
                "values": [
                  {
                    "stringValue": "{{.RandomIPv4}}"
                  },
                  {
                    "stringValue": "{{.RandomIPv6}}"
                  }
                ]
              }
            }
          },
          {
            "key": "host.mac",
            "value": {
              "arrayValue": {
                "values": [
                  {
                    "stringValue": "{{.RandomMAC}}"
                  },
                  {
                    "stringValue": "{{.RandomMAC}}"
                  }
                ]
              }
            }
          }
        ]
      },
      "schemaUrl": "https://opentelemetry.io/schemas/1.9.0"
    }
  ]
}
